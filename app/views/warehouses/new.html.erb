<div class="banner" style="background-image: linear-gradient(-225deg, rgba(0,101,168,0.6) 0%, rgba(0,36,61,0.6) 50%), url(https://images.unsplash.com/photo-1484856275728-97e5c0381ec0?ixlib=rb-0.3.5&s=15d7fe32eb58cc6c1b06681fa666b078&auto=format&fit=crop&w=1049&q=80);">
  <div class="banner-content">
    <div class="container">
      <%= simple_form_for(@warehouse)  do |f| %>
        <%= f.input :address %>
        <%= f.input :surface %>
        <%= f.input :pallets %>
        <%= f.input :description %>
        <%= f.input :public_price %>
        <%= f.input :food_grade_certified %>
        <%= f.input :frozen_certified %>
        <%= f.input :alcohol_certified %>
        <%= f.input :fulfillment_services %>
        <%= f.input :transportation_services %>
        <%= f.input :devanning_services %>
        <%= f.submit "add your warehouse", class: "btn btn-primary" %>
      <% end %>
    </div>
  </div>
</div>




